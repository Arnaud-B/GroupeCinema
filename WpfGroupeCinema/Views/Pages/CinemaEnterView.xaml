<Page x:Class="WpfGroupeCinema.Views.CinemaEnterView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfGroupeCinema.Views"
        xmlns:userControl="clr-namespace:WpfGroupeCinema.CinemaListUserControl"
        mc:Ignorable="d"
        Title="CinemaEnterView">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <userControl:OwnerUserControl x:Name="ownerUserControl" Grid.Row="0" />
        <Button x:Name="myBtn" Grid.Row="3" Content="ClickMe"/>
        <Button x:Name="BtnNavigate1" Grid.Row="4" Content="MENU" Click="BtnNavigate1_Click"/>
    </Grid>
</Page>
