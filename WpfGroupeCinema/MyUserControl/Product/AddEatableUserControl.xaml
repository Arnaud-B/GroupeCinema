<baseUserControl:BaseUserControl
             x:Class="WpfGroupeCinema.CinemaListUserControl.AddEatableUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfGroupeCinema.MyUserControl"
             xmlns:baseUserControl="clr-namespace:WpfGroupeCinema.CinemaListUserControl"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox Grid.Row="0" Grid.Column="0" x:Name="eatableNameTxtB" Text="Name"/>
        <TextBox Grid.Row="1" Grid.Column="0" x:Name="eatablePriceTxtB" Text="Price" />
        <TextBox Grid.Row="2" Grid.Column="0" x:Name="eatableWeightTxtB" Text="Weight" />
        <TextBox Grid.Row="3" Grid.Column="0" x:Name="eatableNumberTxtB" Text="Number" />
    </Grid>
</baseUserControl:BaseUserControl>
